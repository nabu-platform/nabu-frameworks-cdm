<structure name="pipeline">
    <structure name="input">
        <field comment="The cdm type id" name="typeId" type="java.lang.String"/>
        <field comment="The item was created in _a_ system, we need to pull that first as it needs to form the basis of further enrichment from other providers" name="system" type="java.lang.String"/>
        <structure comment="Any external ids you already know. Other systems will need as much information as possible about the entity." definition="nabu.frameworks.cdm.specs.externalId" maxOccurs="unbounded" name="externalIds" restrict="cdmInstanceId, systemId, id"/>
    </structure>
    <structure name="output">
        <structure collectionCrudProvider="odata" collectionName="teamsTemplates" definition="java.lang.Object" name="cdmInstance"/>
    </structure>
    <structure collectionName="cdmDefinitions" definition="nabu.frameworks.cdm.crud.cdmDefinition.types.output" minOccurs="0" name="definition"/>
    <field maxOccurs="unbounded" minOccurs="0" name="systemNames" type="java.lang.String"/>
    <structure collectionName="cdmSystems" definition="nabu.frameworks.cdm.crud.cdmSystem.types.output" maxOccurs="unbounded" minOccurs="0" name="systems"/>
    <structure collectionName="cdmInstances" definition="nabu.frameworks.cdm.crud.cdmInstance.types.output" name="instance"/>
    <structure definition="nabu.utils.types.ParameterDescription" maxOccurs="unbounded" minOccurs="0" name="parameters"/>
    <structure definition="nabu.utils.types.ParameterDescription" minOccurs="0" name="primaryKey"/>
</structure>
